<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <TextView
        android:id="@+id/tv_sdk_jg"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="极光推送"
        android:textColor="@color/color_black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_sdk_um_share"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_sdk_um_share"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="友盟分享,登录"
        android:textColor="@color/color_black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_sdk_um_bugly"
        app:layout_constraintTop_toBottomOf="@+id/tv_sdk_jg" />


    <TextView
        android:id="@+id/tv_sdk_um_bugly"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="友盟统计，腾讯bugly"
        android:textColor="@color/color_black"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_sdk_pay"
        app:layout_constraintTop_toBottomOf="@+id/tv_sdk_um_share" />


    <TextView
        android:id="@+id/tv_sdk_pay"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:text="微信 支付宝支付"
        android:textColor="@color/color_black"
        android:textSize="16sp"
        app:layout_constraintTop_toBottomOf="@+id/tv_sdk_um_bugly" />


</androidx.constraintlayout.widget.ConstraintLayout>